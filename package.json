    "eject": "react-scripts eject",
    "eslint-fix": "eslint --fix \"**/*.js\" \"**/*.jsx\"",
    "prettier-write": "prettier --write \"./**/*.json\" \"./**/*.md\" \"./**/*.html\" \"./**/*.css\" \"./**/*.scss\" --config ./.prettierrc.json --ignore-path ./.prettierignore",
